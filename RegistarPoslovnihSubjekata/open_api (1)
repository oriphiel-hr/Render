            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "no_data_error",
            "in": "query",
            "description": "(Nije obavezno) Zadaje da li zahtjev koji nije pronašao tražene podatke vraća grešku (1) ili prazni objekt (0), ako se ne navede vraća se greška",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "omit_nulls",
            "in": "query",
            "description": "(Nije obavezno) Zadaje da li se prazni (null) ključevi izostavljaju iz vraćenih podataka, ako se ne navede prazni ključevi se izostavljaju",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "headers": {
              "X-Snapshot-Id":{
                "description": "Snapshot_id (ID seta) podataka koji se koristi za odgovor",
                "schema": {
                  "type": "integer"
                }
              },
              "X-Timestamp":{
                "description": "Datum i vrijeme kopiranja seta podataka koji se koristi za odgovor",
                "schema": {
                  "type": "string"
                }
              },
              "X-Total-Count":{
                "description": "Ukupni broj redaka u tablici, koristi se za paging",
                "schema": {
                  "type": "integer"
                }
              },
              "X-Seconds-Elapsed":{
                "description": "Vrijeme koje je potrošeno za dohvat podataka, ne uključuje vrijeme prijenosa preko mreže",
                "schema": {
                  "type": "number"
                }
              },
              "X-Rows-Returned":{
                "description": "Broj redaka vraćenih u odgovoru",
                "schema": {
                  "type": "integer"
                }
              },
              "X-Log-Id":{
                "description": "ID log zapisa za odgovor, koristi se u komunikaciji sa službom za korisnike",
                "schema": {
                  "type": "integer"
                }
              }
            },
            "content": {
              "application/json": {
                "schema": {
