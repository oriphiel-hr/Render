{
  "name": "uslugar-backend",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "dev:win": "set NODE_ENV=development && node src/server.js",
    "dev:unix": "NODE_ENV=development node src/server.js",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "seed": "node prisma/seed.js",
    "seed:legal": "node prisma/seed-legal-statuses-only.js",
    "seed:documentation": "node prisma/seeds/seed-documentation.js",
    "migrate-and-seed:documentation": "prisma migrate deploy --schema=prisma/schema.prisma && npm run seed:documentation",
    "extract:documentation": "node scripts/extract-documentation.js",
    "generate:documentation-seed": "node scripts/generate-documentation-seed.js",
    "db:fix": "node prisma/seed-db-fix.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.13.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.12.0",
    "prisma": "5.22.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "tesseract.js": "^6.0.1",
    "twilio": "^5.10.4",
    "openai": "^4.20.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  }
}
